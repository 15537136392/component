.main_nemu{width:250px; height:500px; background: #fff;z-index: 9999;border-right:3px solid #0061BA; overflow-y:auto;}
.content{width: 600px; height: 420px;float:right}
.content iframe{overflow-x:hidden; }
.i-collapse{color:#333;font-weight:700;cursor: pointer;}
.zx-popup-tree .ui-dialog-content { overflow-y:auto; overflow-x:hidden; }
