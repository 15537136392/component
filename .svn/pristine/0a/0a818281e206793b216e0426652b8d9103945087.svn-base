.header{width:100%; background:#0061BA;height:50px;line-height:50px; overflow: hidden; font-family: "\5fae\8f6f\96c5\9ed1";}
.header a{display:block;font-size:18px;font-weight:800;text-indent:50px;color:#fff;,Arial,Tahoma;width: 220px;}
.header a:hover{color: #fff;text-decoration: none;}
li{font-size:12px; color:#999;line-height:24px;padding: 0px;margin: 0px; list-style: none;}
ul{padding: 0px;margin: 6px 0px 2px 16px;}
a{color: #666666; text-decoration: none;}
a:hover{color:#0061BA; text-decoration: underline; }
