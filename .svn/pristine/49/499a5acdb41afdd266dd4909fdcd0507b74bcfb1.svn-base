<body id="main_body" onload="prettyPrint()" >
<div id="J_select"></div>
<div id="J_selectV2"></div>


<pre class="prettyprint mg_t_20 fn-clear" id="html">
&lt;div id=&quot;J_select&quot;&gt;&lt;/div&gt;

&lt;div id=&quot;J_selectV2&quot;&gt;&lt;/div&gt;

$("#J_select").NiceSelect({
	url:"../data/selectDatas.json",
	datas:"codeType=test",
	id:"testId",
	name:"testId",
	defaultValue:"50",
	handler:function(){
		var txt = $(this).val();
		alert("change:" + txt);
	},
	callback:function(){
		alert("callback");
	}
});

$("#J_selectV2").NiceSelect({
	url:"../data/selectDatas.json",
	datas:"codeType=test",
