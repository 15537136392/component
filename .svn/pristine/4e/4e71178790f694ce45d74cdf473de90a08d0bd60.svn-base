}
.TypeList dl:after {
	display:block;
	height:0;
	visibility:hidden;
	clear:both;
	content:".";
}
.searchType dl dt {
	display:inline;
	width:70px;
	float:left;
	padding-top:3px;
	text-align:left;
	clear:left;
	line-height:28px;
	color:#888;
	font-weight:bold;
}
.searchType dl dd {
	display:inline;
	float:left;
}
.searchType dl dd ul {
	display:inline;
	float:left;
	padding:2px 0;
	margin-left:-6px;
	list-style:none;
}
.searchType dl dd ul li {
	display:inline;
	float:left;
	text-overflow:ellipsis;
	overflow:hidden;
	line-height:1.231;
}
.searchType dl dd ul li a {
	display:block;
	color:#666;
	cursor:pointer;
	margin:4px 6px;
	padding:5px 10px 5px 10px;
	white-space:nowrap;
	text-decoration:none;
}
.searchType dl dd ul li a:hover {
	color:#dd6800;
	text-decoration:underline;
}
.searchType dl dd ul li a.click {
	background:#3A8BE0;
	color:#FFF;
	text-decoration:none;
}
