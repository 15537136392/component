    <script type="text/javascript">
    var DraggerTop;
    $(function(){
        // [start] Demo Code //
        var $chapters = $('.leftnav').children('li');
        var $chScrollPositions = new Array();
        // Cache Scroll Positions for Each Chapter
        $chapters.each(function(i){
          $chScrollPositions[i] = Math.round($(this).offset().top - $('.leftnav').offset().top) - 10;
        });
        //======start左侧导航定位
        var $leftpicPositions=new Array();
        var $leftpic=$('.leftpic').children("li");
        $($leftpic[0]).append('<i class="start-cycle"></i>');
        $($leftpic[$leftpic.length-1]).append('<i class="end-cycle"></i>');//给导航加圆圈
        $leftpic.each(function(i){
          $leftpicPositions[i]=Math.round($(this).offset().top - $('.leftpic').offset().top) + 40;
        });
       // console.log($leftpicPositions);
        //======end
        $chapters.eq(0).addClass('active'); // Set First Chapter Active on Start
        
        $('.leftpic > li').click(function(){
          var last = $chapters.parent().find('li.active').removeClass('active').index();
          var next=$(this).index();
          //====
          $('.guide').animate({'top':$leftpicPositions[next]});
          //====
          $chapters.eq(next).addClass('active'); // Set Next Chapter Active
          $(".leftnav").stop().mCustomScrollbar("scrollTo",$chScrollPositions[next]);
        });
      // [end] Demo Code //
         CustomScrollbar($leftpicPositions);
        $('.leftnav a.coldecor').each(function(i,obj){
            $(obj).bind('click',function(){
                var top = Math.round($(this).offset().top - $('.leftpic').offset().top)-10;
                if(DraggerTop)
                 $(".leftnav").mCustomScrollbar("scrollTo",DraggerTop+top-8);
                  else
                $(".leftnav").mCustomScrollbar("scrollTo",top);
            })
        })
    });
        function resetFrameWidth(){
          var sidebarWidth=eval($('.warper').outerWidth(true)+$('.leftnav').outerWidth(true));
          var winW=$(window).width();
          var winH=$(window).height();
          var heardH=$('.norm_top').height();
          var conH=eval(winH-heardH);
          $('.ceniframe').height(conH-4);
          $('.warper').height(conH);
          $('.leftnav').height(conH-20);
          var frameW=eval(winW-sidebarWidth)+"px";
          $('.centercode').width(frameW);
        }
        $(document).ready(function(){
            resetFrameWidth();
            $(window).resize(function(){
                resetFrameWidth();
              });
            addNavSrc();
            updASrc();
      
        });
        var CustomScrollbar=function(arrayObj){
            var $leftnavList = $('#leftnav').children('li');
            var $leftnavPositions = new Array();
            $leftnavList.each(function(i){
                $leftnavPositions[i]=Math.round($(this).offset().top - $('.leftpic').offset().top);
            })
            //===滚动条
            var index=0;
             $(".leftnav").mCustomScrollbar({
                scrollButtons:{
                  enable:true
                },
                scrollButtons:{
                  enable:false
                },
                callbacks:{
                  onScroll:function(){
                    var draggerTop=-this.mcs.top;
                    DraggerTop=-this.mcs.top;
                    for(var i=$leftnavPositions.length-1;i>=0;i--){
                      if(draggerTop+100 >= $leftnavPositions[i]){
                        if(index!=i){
                            index=i;
                            var frameSrcTemp=$('#leftpic>li').eq(index).attr("src");
                            if(frameSrcTemp)
                            $('#iframecontent').attr('src',frameSrcTemp);
                          }
                        break;
                      }
                    }
                    $('.guide').animate({'top':arrayObj[index]});
                  },
                  whileScrolling:function(){
                    DraggerTop=-this.mcs.top;
                  }
                }
            });
        }
        //给.leftnav里边链接绑定事件,并指向iframe锚点
        function updASrc(){
          $('.leftnav a.coldecor').each(function(i,obj){
            var _href=$(obj).attr('href');
            var anchorNM=_href.split("#")[1];
            var path='';
            if(anchorNM!=''){
              $(obj).attr('target','iframename');
              $(obj).bind('click',function(){
                 var frameSrc=$(this).parents('.navigator').attr('src');
                 anchorNM=$(obj).attr('href').split("#")[1];
                 var realHref;
                 if(anchorNM){
                	 realHref=path+frameSrc+"#"+anchorNM;
                 }else{
                	 realHref=$(obj).attr('href');
                 }
                 $(this).attr('href',realHref);
              });
            }
          });
        }
        //给.leftpic导航按钮绑定事件
        function addNavSrc(){
          $('.leftpic li.coldecor').each(function(i,obj){
              $(obj).bind('click',function(){
                var frameSrc=$(this).attr('src');
                if(frameSrc){
               	 $('#iframecontent').attr('src',frameSrc);
                }else{
                	var href=$('.leftnav li').eq(i).find('.navborder a.coldecor:eq(0)').attr('href');
                	 $('#iframecontent').attr('src',href);
                }
              });
          });
        }

        //scroll
        function scrollTarget(){
          var frameDom=getIFrameDOM("iframecontent");
          var docFrame=frameDom.getElementById('_Toc425519468').style.background="red";
        //var docFrame=window.frames["iframecontent"].document.getElementById('_Toc425519468').style.background="red";
          
        }
        function _scrollTo(){
          var frameDom=getIFrameDOM("iframecontent");
          var docFrame=frameDom.getElementById('hello');
          var frameSelf=document.getElementById('iframecontent').contentWindow.document.body;
     	
        }
         function getIFrameDOM(iID){
            return document.getElementById(iID).contentWindow.document;
        }
    </script>



